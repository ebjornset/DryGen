{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "README.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "CNAME",
          "README.md",
          "images/**"
        ]
      }
    ],
    "output": "../_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "globalMetadata": {
      "_appName": "DryGen",
      "_appTitle": "DryGen",
      "_appLogoPath": "images/drygen-compressed.svg",
      "_appFaviconPath": "images/favicon-32x32.png",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": true,
      "_disableBreadcrumb": true,
      "_lang": "en",
      "pdf": false
    },
    "sitemap": {
      "baseUrl": "https://docs.drygen.dev",
      "priority": 0.5,
      "changefreq": "daily"
    }
  }
}